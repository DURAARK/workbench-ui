{{#if isLoading}}
	<li class="da-file-entry"><div class="da-valid btn btn-blue" title="Schema">{{loading-spinner}}</div>Semantic enrichment triple</li>
{{else}}
	<li class="da-file-entry" {{action "onClick" item}}>
		<div style="position: relative;">
			{{#each entry in item.modelArray}}
				<div class="da-valid btn btn-blue">{{entry.key}}</div>{{entry.value.streetAddress}}
				<div {{action "onDelete" item}} style="color: white;" class="btn btn-red btn-tiny btn-topright" title="Delete Session">x</div>
			{{/each}}
		</div>
	</li>
{{/if}}