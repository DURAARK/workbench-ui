{{#if isLoading}}
	<li class="da-file-entry"><div class="da-valid btn btn-alt" title="Schema">{{loading-spinner}}</div>Geometric enrichment item</li>
{{else}}
	<li class="da-file-entry" {{action "onClick" item}}>
		<div style="position: relative;">
				<div class="da-valid btn btn-alt">{{application}}</div>Geometric enrichment item
				<div {{action "onDelete" item}} style="color: white;" class="btn btn-red btn-tiny btn-topright" title="Delete Session">x</div>
		</div>
	</li>
{{/if}}