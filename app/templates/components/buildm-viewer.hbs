<!--List Container-->
<ul class="dropdown-list-container">

  <!--First List-->
  <li class="dropdown-list dropdown-list-no-round-corners">
    <!--List Title-->
    <a dropdown-prop="title">Building Details<i class="dropdown-icon"></i></a>
    <!--List Items-->
    <div class="dropdown-list-items dropdown-list-open dropdownlist-grid">
      <fieldset>
           {{#each item in physicalAsset}}
            <div class="dropdownlist-grid-small">
              <label>{{item.key}}:</label>
              {{#each value in item.values}}
                {{input readonly=true value=value}}
              {{/each}}
            </div>
          {{/each}}
      </fieldset>
    </div>
  </li>

  <!--Second List-->
  <li class="dropdown-list">
    <!--List Title-->
    <a dropdown-prop="title">Plan Details<i class="dropdown-icon"></i></a>
    <!--List Items-->
    <div class="dropdown-list-items dropdownlist-grid">
      <fieldset>
      {{#each item in digitalObject}}
        <div class="dropdownlist-grid-small">
              <label>{{item.key}}:</label>
              {{#each value in item.values}}
                {{input readonly=true value=value}}
              {{/each}}
            </div>
      {{/each}}
      </fieldset>
    </div>
  </li>

