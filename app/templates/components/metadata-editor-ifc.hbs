<!--List Container-->
<ul class="dropdown-list-container">

  <!--Second List-->
  <li class="dropdown-list">
    <!--List Title-->
    <a dropdown-prop="title">IFC Plan Metadata</a>
    <!--List Items-->
    <div class="dropdown-list-items dropdown-list-open">
      <fieldset>

      {{#each item in formDescription}}

        <label>{{item.key}}:</label>

        {{#each value in item.values}}

          {{input readonly=false value=value}}

        {{/each}}

      {{/each}}

      </fieldset>
    </div>
  </li>

</ul>
