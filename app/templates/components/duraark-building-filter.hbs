<div class="c-duraark-building-filter">

  <ul>
    {{#each addressCountryItems as |item|}}
      <li>{{item}}</li>
    {{/each}}
  </ul>
  {{#each-in buildings as |uri building|}}
    {{building-list-item uri=uri model=building showDetailsClicked="showDetails" openBuildingClicked="openBuilding"}}
  {{/each-in}}

  <button {{action "filterChanged"}}>Update</button>
</div>
