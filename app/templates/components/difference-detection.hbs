<div class="l-duraark-ifc-viewer c-duraark-ifc-viewer">
  {{#if tool.hasError}}
    <center>
      <h1>{{tool.errorText}}</h1>

      <button {{action "restartTool"}} type="button" class="c-file__actions__button">Restart Tool</button>
    </center>


  {{else}}
    <iframe src={{viewerUrl}} width="100%" height="700px"></iframe>
  {{/if}}
</div>
