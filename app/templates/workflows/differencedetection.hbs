<!-- Start filelist item -->
<div class="c-file-list">
<div class="c-file-list__category">
  <h3>Select two files to compare:</h3>
</div>
<div class="c-file-list__listing">

  <div class="c-file">
    <div class="c-file__header t-file">
      3D Model (IFC)
    </div>
    <div class="c-file__name">
      <h3>Haus30-Scan-2014-05-24.ifc</h3>
      <div class="c-file__size">
        <h4>382 MB</h4>
      </div>
    </div>
    <div class="c-file__actions">
      <div class="c-file__action">
          <i class="fa fa-times"></i>
        </div>
        <div class="c-file__action">
          <i class="fa fa-edit"></i>
        </div>
        <div class="c-file__action">
          <i class="fa fa-eye"></i>
        </div>
    </div>
  </div>

  <div class="c-file">
    <div class="c-file__header t-file">
      Pointcloud (E57)
    </div>
    <div class="c-file__name">
      <h3>Haus30-Scan-2014-05-24.e57</h3>
      <div class="c-file__size">
        <h4>742 MB</h4>
      </div>
    </div>
    <div class="c-file__actions">
      <div class="c-file__action">
          <i class="fa fa-times"></i>
        </div>
        <div class="c-file__action">
          <i class="fa fa-edit"></i>
        </div>
        <div class="c-file__action">
          <i class="fa fa-eye"></i>
        </div>
    </div>
  </div>

  </div>
</div>
<!-- End filelist item -->

<div class="c-button-list">

  <div class="c-button-list__content">

	{{#link-to "workflows.geometricenrichment-added-difference-detection" class="c-button"}}
    <div class="c-button__frame t-button-files">
  		<div class="c-button__name">
  			<h3>Detect</h3>
  		</div>
    </div>
	{{/link-to}}

  {{#link-to "workflows.geometricenrichment" class="c-button"}}
    <div class="c-button__frame t-button-files">
  		<div class="c-button__name">
  			<h3>Cancel</h3>
  		</div>
    </div>
	{{/link-to}}

  </div>
</div>
