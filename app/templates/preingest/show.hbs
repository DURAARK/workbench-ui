<div class="da-session-header">{{name}}</div>
<div style="text-align: center;">{{creator}} / {{createdAt}}</div>

<div class="da-session-next">&#8615;</div>

<div class="da-session-workflow-header">
	<span>Files</span>
</div>

{{files-stage stage=filestage editClicked="editStage"}}

<div class="da-session-workflow-header"></div>
<div class="da-session-next">&#8615;</div>

<div class="da-session-workflow-header">
	<span>Metadata</span>
</div>

{{metadata-stage stage=metadatastage editClicked="editStage"}}

<div class="da-session-workflow-header"></div>
<div class="da-session-next">&#8615;</div>

<div class="da-session-workflow-header">
	<span>Semantic Enrichment</span>
</div>

{{#if hasIfcFile}}
	{{semenrichment-stage stage=semanticenrichmentstage editClicked="editStage"}}
{{else}}
	<br>
	<center>
		<div style="font-size: 20px;">Add an IFC file to enable the semantic-enrichement component.</div>
		<small>(this limitation will be removed in upcoming versions)</small>
	</center>
{{/if}}

<div class="da-session-workflow-header"></div>
<div class="da-session-next">&#8615;</div>

<div class="da-session-workflow-header">
	<span>Geometric Enrichment</span>
</div>

{{#if hasE57File}}
{{log geoenrichmentstage}}
	{{geoenrichment-stage stage=geometricenrichmentstage editClicked="editStage"}}
{{else}}
	<br>
	<center>
		<div style="font-size: 20px;">Add an E57 file to enable the geometric-enrichement component.</div>
	</center>
{{/if}}

<div class="da-session-workflow-header"></div>

<br><br>
{{!-- <div style="margin-left: 44%;"> --}}
<center>
<button {{action "goToPreservation" this}} class="btn btn-green btn-big">CREATE SIP</button>
{{!-- </div> --}}
</center>

